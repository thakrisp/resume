---
import { SITE_TITLE } from "../config";
---

<nav class="sticky top-0 z-20">
  <div
    class="flex flex-row justify-between bg-gray-500 px-2 pb-1 md:rounded-b-md"
  >
    <a href="#" class="flex flex-row">
      <img
        src="/resume/RC_Icon.png"
        class="md:hidden"
        alt="Ryan Crispin Logo"
      />
      <span class="self-center invisible md:visible font-semibold text-white"
        >RyanCrispin.me</span
      >
    </a>
    <div class="self-center">
      <ul class="flex flex-row items-end space-x-2">
        <li class="underline underline-offset-2">
          <a href="#" class="" aria-current="page">Home</a>
        </li>
        <li class="underline underline-offset-2">
          <a
            href="https://www.linkedin.com/in/ryancrispin/"
            class="flex"
            aria-current="page"
          >
            <img
              class="pr-0.5"
              alt="svgImg"
              src="data:image/svg+xml;base64,"
            />
            LinkedIn</a
          >
        </li>
        <li class="underline underline-offset-2">
          <a
            href="https://github.com/thakrisp"
            class="flex flex-row"
            aria-current="page"
          >
            <img
              alt="svgImg"
              src="data:image/svg+xml;base64,"
            />GitHub</a
          >
        </li>
      </ul>
    </div>
  </div>
</nav>

